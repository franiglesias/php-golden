{
  "name": "franiglesias/golden",
  "description": "A library for snapshot testing inPHP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fran Iglesias",
      "email": "thetalkingbit@gmail.com",
      "homepage": "https://franiglesias.github.io",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FranIglesias\\Golden\\": "src"
    },
    "files": [
      "src/Options.php"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "require": {
    "php": "7.4.* || >=8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "11.*",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\FranIglesias\\Golden\\": "tests"
    }
  }
}
